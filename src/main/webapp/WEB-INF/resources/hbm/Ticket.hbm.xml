<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.krishagni.crm.domain.Ticket" table="TICKET">
    <id name="jiraId" column="JIRA_ID"/>
    
    <property name="resolveDate" column="RESOLVED_DATE"/>
    
    <property name="summary" column="SUMMARY"/>
  </class>
</hibernate-mapping>